    "type": "library",
    "description": "A lax newsfeed parser",
    "keywords": ["rss","atom","jsonfeed"],
    "license": "MIT",
    "authors": [
        {
            "name": "J. King",
            "email": "jking@jkingweb.ca",
            "homepage": "https://jkingweb.ca/"
        }

    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "sabre/uri": "^2.0",
        "ralouphie/mimey": "^2.1",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "JKingWeb\\Lax\\": "lib/"
        }
